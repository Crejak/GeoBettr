{
  "manifest_version": 2,
  "name": "Geobettr",
  "version": "0.1",
  "description": "Enhance Geoguessr UI",
  "icons": {
    "48": "res/img/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.geoguessr.com/*"
      ],
      "js": [
        "bundle.js"
      ],
      "css": [
        "res/css/main.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "res/img/*"
  ]
}
  